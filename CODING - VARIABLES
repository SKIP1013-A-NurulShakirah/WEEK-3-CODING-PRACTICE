package numeric_data;


//Excercise 1 : Add five integers and display their sum


public class Excercise1 { 

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		//Declare five numbers using int
		int num1 = 11, num2 = 22, num3 = 33, num4 = 44, num5 = 55;
		
		//Declare next two numbers using double
		double num6 = 57, num7 = 38;
		
		//Declare next two numbers using float
		float num8 = 99, num9 = 200;
		
		//Declare sum, average, divide, float
		int sum, avg, div;
		double div1, mul;
		float div2, mul1;
		
		sum = num1 + num2 + num3 + num4 + num5;
		avg = sum / 5;
		div = num5 / num2;
		div1 = num7 / num6;
		div2 = num9 / num8;
		mul = num1 * num2 * num3 * num4 * num5 * num6 * num7 * num8 * num9;
		mul1 = (num1 * num2) / num3;
		
		System.out.println("The sum is " +sum);
		System.out.println();
		
		System.out.println("The average is " +avg);
		System.out.println();
		
		System.out.println("The divide of int number is " +div);
		System.out.println();
		
		System.out.println("The divide of double number is " +div1);
		
		System.out.printf("\nDivide double number : %.2f", div1);
		System.out.println();
		
		System.out.printf("\nDivide float number  : %.2f ", div2);
		System.out.println();
		
		System.out.println("\nThe multiplication of number is " +mul);
		
		System.out.printf("\nThe multiplication of number is %.2f", mul1);
		
	}

}
